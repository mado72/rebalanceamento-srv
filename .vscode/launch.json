{
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "nodemon",
      "program": "${workspaceFolder}/src/index.js",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "nodemon",
      "skipFiles": [
          "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Debug via npm",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/src/",
      "runtimeExecutable": "npm",
      "env": {
        "DEBUG": "true",
      },
      "runtimeArgs": [
          "run-script",
          "debug"
      ]
}//E:\Desenv\finfor\rebalanceamento\srv-rebalanceamento\.vscode\launch.json
  ]
}
